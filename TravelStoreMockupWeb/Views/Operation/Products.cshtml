@RenderPage("_NavOperations.cshtml")
<div class="grid_16">
    <div class="isotope_holder indented_area">
        <ul class="gallery feature_tiles clearfix">
            <li class="isotope-item"><a class="full_width light large_icon icon_top on_dark dialog_button img_icon"
                data-dialog="create_product">
                <img src="~/Content/lib/images/icons/large/grey/frames.png">
                <span class="name sort_1">Create Product</span> </a></li>
        </ul>
    </div>
</div>
<div class="box grid_16">
    <div class="block lines">
        <form class="validate_form" novalidate="novalidate">
        <h2 class="section">Search</h2>
        <div class="columns clearfix">
            <div class="col_50">
                <fieldset class="label_side label_small top">
                    <label for="text_field_inline">
                        From</label>
                    <div class="clearfix">
                        <input type="text" class="text">
                    </div>
                </fieldset>
            </div>
            <div class="col_50">
                <fieldset class="label_side label_small top right">
                    <label for="text_field_inline">
                        To</label>
                    <div class="clearfix">
                        <input type="text" class="text">
                    </div>
                </fieldset>
            </div>
        </div>
        <div class="columns clearfix">
            <div class="col_50">
                <fieldset class="label_side label_small top">
                    <label for="text_field_inline">
                        From</label>
                    <div class="clearfix">
                        <input type="text" class="datepicker">
                    </div>
                </fieldset>
            </div>
            <div class="col_50">
                <fieldset class="label_side label_small top right">
                    <label for="text_field_inline">
                        To</label>
                    <div class="clearfix">
                        <input type="text" class="datepicker">
                    </div>
                </fieldset>
            </div>
        </div>
        <div class="columns clearfix">
            <div class="col_50">
                <fieldset class="label_side label_small top right">
                    <div class="uniform inline clearfix">
                        <label for="yes_share_room">
                            <input type="checkbox" name="answer4b" id="yes4b" />Share Room</label>
                    </div>
                </fieldset>
            </div>
            <div class="col_50">
                <fieldset class="label_side top">
                    <label>
                        Budget</label>
                    <div>
                        <p>
                            <input type="text" id="amount" style="border: 0; font-weight: bold;" />
                        </p>
                        <div style="width: auto; margin-right: 45px;" id="budgetRange">
                        </div>
                    </div>
                </fieldset>
            </div>
        </div>
        <div class="button_bar clearfix">
            <button class="grey dark">
                <img src="~/Content/lib/images/icons/small/white/magnifying_glass.png">
                <span>Search</span>
            </button>
        </div>
        </form>
    </div>
</div>

