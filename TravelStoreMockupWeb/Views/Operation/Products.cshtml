@section scripts{
    <script>
        $(function () {
            $("#budgetRange").slider({
                range: true,
                min: 0,
                max: 80000,
                step: 1000,
                values: [0, 80000],
                slide: function (event, ui) {
                    $("#amount").val("฿" + ui.values[0] + " - ฿" + ui.values[1]);
                }
            });

            $("#amount").val("฿" + $("#budgetRange").slider("values", 0) +
            " - ฿" + $("#budgetRange").slider("values", 1));
        });
    </script>
}
